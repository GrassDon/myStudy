<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person" class="com.study.Person">
        <property name="name" value="dong"></property>
        <property name="sex" value="male"></property>
    </bean>

    <bean id="person2" class="com.study.Person">
        <property name="name" value="lin"></property>
        <property name="sex" value="female"></property>
    </bean>

</beans>